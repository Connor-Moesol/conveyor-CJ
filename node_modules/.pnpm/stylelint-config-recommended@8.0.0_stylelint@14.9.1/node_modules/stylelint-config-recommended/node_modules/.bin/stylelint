#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/rhernandez/xm/moebius-ui-starter/node_modules/.pnpm/stylelint@14.9.1/node_modules/stylelint/bin/node_modules:/home/rhernandez/xm/moebius-ui-starter/node_modules/.pnpm/stylelint@14.9.1/node_modules/stylelint/node_modules:/home/rhernandez/xm/moebius-ui-starter/node_modules/.pnpm/stylelint@14.9.1/node_modules:/home/rhernandez/xm/moebius-ui-starter/node_modules/.pnpm/node_modules:/home/rhernandez/xm/moebius-ui-starter/node_modules:/home/rhernandez/xm/node_modules:/home/rhernandez/node_modules:/home/node_modules:/node_modules:/home/rhernandez/xm/moebius-ui-starter/node_modules/.pnpm/stylelint-config-recommended@8.0.0_stylelint@14.9.1/node_modules/stylelint-config-recommended/node_modules:/home/rhernandez/xm/moebius-ui-starter/node_modules/.pnpm/stylelint-config-recommended@8.0.0_stylelint@14.9.1/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/rhernandez/xm/moebius-ui-starter/node_modules/.pnpm/stylelint@14.9.1/node_modules/stylelint/bin/node_modules:/home/rhernandez/xm/moebius-ui-starter/node_modules/.pnpm/stylelint@14.9.1/node_modules/stylelint/node_modules:/home/rhernandez/xm/moebius-ui-starter/node_modules/.pnpm/stylelint@14.9.1/node_modules:/home/rhernandez/xm/moebius-ui-starter/node_modules/.pnpm/node_modules:/home/rhernandez/xm/moebius-ui-starter/node_modules:/home/rhernandez/xm/node_modules:/home/rhernandez/node_modules:/home/node_modules:/node_modules:/home/rhernandez/xm/moebius-ui-starter/node_modules/.pnpm/stylelint-config-recommended@8.0.0_stylelint@14.9.1/node_modules/stylelint-config-recommended/node_modules:/home/rhernandez/xm/moebius-ui-starter/node_modules/.pnpm/stylelint-config-recommended@8.0.0_stylelint@14.9.1/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../stylelint@14.9.1/node_modules/stylelint/bin/stylelint.js" "$@"
else
  exec node  "$basedir/../../../../../stylelint@14.9.1/node_modules/stylelint/bin/stylelint.js" "$@"
fi
