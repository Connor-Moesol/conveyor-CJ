#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/rhernandez/xm/moebius-ui-starter/node_modules/.pnpm/lint-staged@13.0.3/node_modules/lint-staged/bin/node_modules:/home/rhernandez/xm/moebius-ui-starter/node_modules/.pnpm/lint-staged@13.0.3/node_modules/lint-staged/node_modules:/home/rhernandez/xm/moebius-ui-starter/node_modules/.pnpm/lint-staged@13.0.3/node_modules:/home/rhernandez/xm/moebius-ui-starter/node_modules/.pnpm/node_modules:/home/rhernandez/xm/moebius-ui-starter/node_modules:/home/rhernandez/xm/node_modules:/home/rhernandez/node_modules:/home/node_modules:/node_modules:/home/rhernandez/xm/moebius-ui-starter/node_modules/lint-staged/bin/node_modules:/home/rhernandez/xm/moebius-ui-starter/node_modules/lint-staged/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/rhernandez/xm/moebius-ui-starter/node_modules/.pnpm/lint-staged@13.0.3/node_modules/lint-staged/bin/node_modules:/home/rhernandez/xm/moebius-ui-starter/node_modules/.pnpm/lint-staged@13.0.3/node_modules/lint-staged/node_modules:/home/rhernandez/xm/moebius-ui-starter/node_modules/.pnpm/lint-staged@13.0.3/node_modules:/home/rhernandez/xm/moebius-ui-starter/node_modules/.pnpm/node_modules:/home/rhernandez/xm/moebius-ui-starter/node_modules:/home/rhernandez/xm/node_modules:/home/rhernandez/node_modules:/home/node_modules:/node_modules:/home/rhernandez/xm/moebius-ui-starter/node_modules/lint-staged/bin/node_modules:/home/rhernandez/xm/moebius-ui-starter/node_modules/lint-staged/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../lint-staged/bin/lint-staged.js" "$@"
else
  exec node  "$basedir/../lint-staged/bin/lint-staged.js" "$@"
fi
