#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/rhernandez/xm/moebius-ui-starter/node_modules/.pnpm/vitest@0.16.0_@vitest+ui@0.16.0+jsdom@20.0.0/node_modules/vitest/node_modules:/home/rhernandez/xm/moebius-ui-starter/node_modules/.pnpm/vitest@0.16.0_@vitest+ui@0.16.0+jsdom@20.0.0/node_modules:/home/rhernandez/xm/moebius-ui-starter/node_modules/.pnpm/node_modules:/home/rhernandez/xm/moebius-ui-starter/node_modules:/home/rhernandez/xm/node_modules:/home/rhernandez/node_modules:/home/node_modules:/node_modules:/home/rhernandez/xm/moebius-ui-starter/node_modules/vitest/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/rhernandez/xm/moebius-ui-starter/node_modules/.pnpm/vitest@0.16.0_@vitest+ui@0.16.0+jsdom@20.0.0/node_modules/vitest/node_modules:/home/rhernandez/xm/moebius-ui-starter/node_modules/.pnpm/vitest@0.16.0_@vitest+ui@0.16.0+jsdom@20.0.0/node_modules:/home/rhernandez/xm/moebius-ui-starter/node_modules/.pnpm/node_modules:/home/rhernandez/xm/moebius-ui-starter/node_modules:/home/rhernandez/xm/node_modules:/home/rhernandez/node_modules:/home/node_modules:/node_modules:/home/rhernandez/xm/moebius-ui-starter/node_modules/vitest/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitest/vitest.mjs" "$@"
else
  exec node  "$basedir/../vitest/vitest.mjs" "$@"
fi
