{
  "name": "conveyor",
  "private": true,
  "version": "0.0.0",
  "author": "Moebius Solutions",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "format": "prettier -uw --ignore-path .gitignore .",
    "run-tsc": "tsc",
    "run-eslint": "eslint --cache --fix --ignore-path .gitignore --ext .js,.ts,.tsx",
    "run-stylelint": "stylelint --cache --fix --ignore-path .gitignore '**/*.css'",
    "lint": "run-p run-tsc run-stylelint run-eslint",
    "preview": "vite preview",
    "test": "vitest",
    "test:ci": "vitest run",
    "test:ui": "vitest --ui",
    "validate": "run-p lint test:ci"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.5",
    "@tanstack/react-query": "^4.0.10",
    "@tanstack/react-query-devtools": "^4.0.10",
    "@tanstack/react-table": "^8.5.5",
    "bootstrap": "^5.1.3",
    "bootswatch": "^5.1.3",
    "graphql": "^16.5.0",
    "graphql-request": "^4.3.0",
    "react": "^18.2.0",
    "react-bootstrap": "^2.4.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^6.3.0",
    "react-use": "^17.4.0"
  },
  "devDependencies": {
    "@testing-library/dom": "8.14.0",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.3.0",
    "@testing-library/user-event": "14.2.1",
    "@types/react": "18.0.14",
    "@types/react-dom": "18.0.5",
    "@types/react-helmet": "6.1.5",
    "@types/testing-library__jest-dom": "5.14.5",
    "@typescript-eslint/eslint-plugin": "5.30.0",
    "@typescript-eslint/parser": "5.28.0",
    "@vitejs/plugin-react": "1.3.2",
    "@vitest/ui": "0.16.0",
    "eslint": "8.18.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.1.0",
    "eslint-plugin-react": "7.30.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "jsdom": "20.0.0",
    "lint-staged": "13.0.3",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "stylelint": "14.9.1",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-standard": "26.0.0",
    "typescript": "4.7.4",
    "vite": "3.0.0",
    "vitest": "0.18.1"
  },
  "lint-staged": {
    "*": "prettier -uw --cache",
    "*.css": "stylelint --cache --fix",
    "*.{ts,tsx}": [
      "eslint --cache --fix",
      "vitest related --run --coverage=false"
    ]
  }
}
